- i = @lifelist.size + 1
- @lifelist.chunk {|s| s.first_seen_date.year}.each do |year, species_list|
  - if params[:year].blank?
    %h5.first_seen= t('.first_seen_in', year: year)
  %ol.lifers_list
    - species_list.each do |sp|
      %li.lifer_row{:value => i -= 1}
        = render partial: 'lifer', object: sp
