- @page_title = 'eBird submissions'

%h1= page_header

%table.neat_table
  %tr
    %th id
    %th Name
    %th Created
    %th Cards count
    %th Status

  - @files.each do |file|
    %tr
      %td= file.id
      %td= file.name
      %td= file.created_at.strftime("%F %R")
      %td= file.cards.size
      %td= file.status
